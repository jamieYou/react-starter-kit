{
  "name": "react-mobx-starter-kit",
  "version": "1.1.2",
  "private": true,
  "homepage": "https://github.com/jamieYou/react-mobx-starter-kit",
  "scripts": {
    "start": "npm-run-all --parallel server:dev lint:watch",
    "staging": "npm-run-all remove-dist lint create:dist:staging",
    "build": "npm-run-all remove-dist lint create:dist",
    "start:prod": "npm-run-all build server:prod",
    "server:dev": "nodemon ./script/server.js --watch script --watch config",
    "server:prod": "cross-env NODE_ENV=production node ./script/server.js",
    "create:dist": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js --progress",
    "create:dist:staging": "cross-env NODE_ENV=staging webpack --config ./config/webpack.prod.js --progress",
    "lint": "esw src --ext .jsx --ext .js",
    "lint:watch": "npm run lint -- --watch",
    "remove-dist": "rimraf ./dist"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.3",
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression": "^1.7.2",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-beansmile": "^1.1.0",
    "eslint-watch": "^3.1.5",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "ipv4": "^1.0.4",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "opn": "^5.3.0",
    "postcss-loader": "^2.1.5",
    "postcss-pxtorem": "^4.0.1",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.4",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "antd-mobile": "^2.2.1",
    "babel-polyfill": "^6.26.0",
    "core-decorators": "^0.20.0",
    "core-js": "^2.5.7",
    "fastclick": "^1.0.6",
    "github-markdown-css": "^2.10.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "mobile-detect": "^1.4.2",
    "mobx": "^4.2.1",
    "mobx-logger": "^0.7.0",
    "mobx-react": "^5.1.2",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
